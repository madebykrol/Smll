<?xml version="1.0" encoding="UTF-8"?>
<appSettings>
    <system>
        <installation installed="false" />
    </system>
    <encryption>
        <add name="Default" key="IJhKcFXXl" />
    </encryption>
    <connectionStrings>
        <add name="Default" 
            connectionString="Driver=MySQL;Server=localhost;Database=smll_example;User=user;Password=pass123" />
    </connectionStrings>
    <authentication>
          <add name="Provider" provider="FormAuthentication" />
    </authentication>
    <applicationPool>
        <add name="Default" path="~/assets/files/" />
    </applicationPool>
    <web>
        <membership defaultProvider="DefaultMembershipProvider">
            <providers>
                <add name="DefaultMembershipProvider" type="SqlMembershipProvider" />
            </providers>
        </membership>
        <roleManager defaultProvider="DefaultRoleProvider">
            <providers>
                <add name="DefaultRoleProvider" type="SqlRoleProvider" />
            </providers>
        </roleManager>
        <applicationState state="Debug"></applicationState>
        <cookiePath path="" domain="" />
    </web>
</appSettings>
